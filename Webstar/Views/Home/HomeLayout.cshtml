<!DOCTYPE html>
<!--[if lt IE 7]><html lang="en" class="no-js ie6"><![endif]-->
<!--[if IE 7]><html lang="en" class="no-js ie7"><![endif]-->
<!--[if IE 8]><html lang="en" class="no-js ie8"><![endif]-->
<!--[if gt IE 8]><!-->
<html lang="en" class="no-js">
<!--<![endif]-->

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <title>@ViewBag.Title - My ASP.NET Application</title>

    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />

    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <![endif]-->

</head>

<body>

    <div class="pre-loader">
        <div class="load-con">
            <img src='@Url.Content("~/Content/img/star.png")' class="animated rotateIn" alt="">
            <div class="spinner">
                <div class="bounce1"></div>
                <div class="bounce2"></div>
                <div class="bounce3"></div>
            </div>
        </div>
    </div>

    <header>

        <nav class="navbar navbar-default navbar-fixed-top" role="navigation">
            <div class="container">
                <!-- Brand and toggle get grouped for better mobile display -->
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                        <span class="fa fa-bars fa-lg"></span>
                    </button>
                    <a class="navbar-brand" href='@Url.Action("Index", "Home")'>
                        <img src='@Url.Content("~/Content/img/logo.png")' />
                    </a>
                </div>

                <!-- Collect the nav links, forms, and other content for toggling -->
                <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">

                    <ul class="nav navbar-nav navbar-right">
                        <li>
                            <a data-scroll-nav="0" href="">services</a>
                        </li>
                        <li>
                            <a data-scroll-nav="1" href="">skills</a>
                        </li>
                        <li>
                            <a data-scroll-nav="2" href="">work</a>
                        </li>
                        <li>
                            <a data-scroll-nav="3" href="">contact</a>
                        </li>
                        <li>
                            <a href='@Url.Action("Login", "Account")'>Admin</a>
                        </li>
                    </ul>
                </div>
                <!-- /.navbar-collapse -->
            </div>
            <!-- /.container-->
        </nav>


        <section id="splash">


                <div id="sequence">

                    <img class="sequence-prev" src='@Url.Content("~/Content/img/sequence/bt-prev.png")' alt="Previous Frame" />
                    <img class="sequence-next" src='@Url.Content("~/Content/img/sequence/bt-next.png")' alt="Next Frame" />

                    <ul class="sequence-canvas">
                        <li>
                            <div class="bg-map">
                                <div class="pos">
                                    <img class="model" src='@Url.Content("~/Content/img/sequence/model1.png")' alt="Model 1" />
                                    <div class="display">
                                        <h2 class="title">Web Technology Standards</h2>
                                        <h3 class="subtitle">Specializing in HTML5, CSS3, Javascript, C# and ASP.NET MVC</h3>
                                    </div>
                                </div>
                            </div>
                        </li>
                        <li>
                            <div class="bg-plan">
                                <div class="pos">
                                    <img class="model" src='@Url.Content("~/Content/img/sequence/model2.png")' alt="Model 2" />
                                    <div class="display">
                                        <h2 class="title">Strategic Planning</h2>
                                        <h3 class="subtitle">Analysis, Reporting, Positioning</h3>
                                    </div>
                                </div>
                            </div>
                        </li>
                        <li>
                            <div class="bg-phone">
                                <div class="pos">
                                    <img class="model" src='@Url.Content("~/Content/img/sequence/model3.png")' alt="Model 3" />
                                    <div class="display">
                                        <h2 class="title">Mobile Development</h2>
                                        <h3 class="subtitle">Responsive Design, Cloud Services, Touch Enabled</h3>
                                    </div>
                                </div>
                            </div>
                        </li>
                    </ul>

                    <ul class="sequence-pagination">
                        <li><img src='@Url.Content("~/Content/img/sequence/tn-model1.png")' alt="Model 1" /></li>
                        <li><img src='@Url.Content("~/Content/img/sequence/tn-model2.png")' alt="Model 2" /></li>
                        <li><img src='@Url.Content("~/Content/img/sequence/tn-model3.png")' alt="Model 3" /></li>
                    </ul>

                </div>
        </section>

    </header>

    <div class="wrapper">

        @RenderBody()

        <footer>
            <div class="container">
                <a href="#" class="wow fadeInDown">
                    <img src='@Url.Content("~/Content/img/logo.png")' alt="" class="logo">
                </a>
                <div class="social">
                    <a href="#" class="wow fadeInDown"><i class="fa fa-twitter fa-lg"></i></a>
                    <a href="#" class="wow fadeInDown"><i class="fa fa-google-plus fa-lg"></i></a>
                    <a href="#" class="wow fadeInDown"><i class="fa fa-facebook fa-lg"></i></a>
                </div>
                <div class="rights">
                    <p>&copy; @DateTime.Now.Year - WEBSTAR DEVELOPEMENT LLC</p>
                    <p>
                        147 Hood Jordan Road, Pelzer, SC 29669 /
                        <a href="mailto:bryan@thewebstar.net">bryan@thewebstar.net</a> /
                        +1 864-608-0824
                    </p>
                </div>
            </div>
        </footer>

    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/customjs")
    @RenderSection("scripts", required: false)

    <script>
        $(document).ready(function() {
            appMaster.preLoader();


        });
    </script>

</body>

</html>
