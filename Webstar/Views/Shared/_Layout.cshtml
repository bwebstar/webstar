<!doctype html>
<!--[if lt IE 7]><html lang="en" class="no-js ie6"><![endif]-->
<!--[if IE 7]><html lang="en" class="no-js ie7"><![endif]-->
<!--[if IE 8]><html lang="en" class="no-js ie8"><![endif]-->
<!--[if gt IE 8]><!-->
<html lang="en" class="no-js">
<!--<![endif]-->
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <![endif]-->
</head>

<body>

    <header class="subpage">

        <nav class="navbar navbar-default navbar-fixed-top" role="navigation">
            <div class="container">
                <!-- Brand and toggle get grouped for better mobile display -->
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                        <span class="fa fa-bars fa-lg"></span>
                    </button>
                    <a class="navbar-brand" href='@Url.Action("Index", "Home")'>
                        <img src='@Url.Content("~/Content/img/logo.png")' />
                    </a>
                </div>

                <!-- Collect the nav links, forms, and other content for toggling -->
                <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">

                    <ul class="nav navbar-nav navbar-right">
                        @Html.Partial("_LoginPartial")
                    </ul>
                </div>
                <!-- /.navbar-collapse -->
            </div>
            <!-- /.container-->
        </nav>

    </header>

    <div class="wrapper">

        <section id="main">
            <div class="container">

                @RenderBody()

            </div>
        </section>

        <footer>
            <div class="container">
                <a href="#" class="wow fadeInDown">
                    <img src='@Url.Content("~/Content/img/logo.png")' alt="" class="logo">
                </a>
                <div class="social">
                    <a href="#" class="wow fadeInDown"><i class="fa fa-twitter fa-lg"></i></a>
                    <a href="#" class="wow fadeInDown"><i class="fa fa-google-plus fa-lg"></i></a>
                    <a href="#" class="wow fadeInDown"><i class="fa fa-facebook fa-lg"></i></a>
                </div>
                <div class="rights">
                    <p>&copy; @DateTime.Now.Year - WEBSTAR DEVELOPEMENT LLC</p>
                    <p>
                        505 S. Orchard Farms Ave., Simpsonville, SC 29681 /
                        <a href="mailto:bryan@thewebstar.net">bryan@thewebstar.net</a> /
                        +1 864-608-0824
                    </p>
                </div>
            </div>
        </footer>

    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/customjs")
    @RenderSection("scripts", required: false)

    <script>
        $(document).ready(function() {
            appMaster.preLoader();
        });
    </script>

</body>

</html>